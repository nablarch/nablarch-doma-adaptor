<job id="dbTest-batchlet-test" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">
  <listeners>
    <listener ref="nablarchJobListenerExecutor">
      <properties>
        <property name="diConfigFilePath" value="integration-test/jbatch.xml" />
      </properties>
    </listener>
  </listeners>

  <step id="step1">
    <listeners>
      <listener ref="nablarchStepListenerExecutor" />
    </listeners>
    <batchlet ref="dbTestBatchlet">
        <properties>
          <property name="isError" value="false"/>
        </properties>
    </batchlet>
  </step>
</job>